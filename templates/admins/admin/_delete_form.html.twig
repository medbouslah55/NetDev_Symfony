<form method="post" action="{{ path('admin_delete', {'cin': admin.cin}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin.cin) }}">
    <button class="btn"><i class="align-middle" data-feather="trash"></i></button>
</form>
